{
  "name": "react-example",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": ""
  },
  "description": "Manage development workflow of react with webpack.",
  "main": "server.js",
  "scripts": {
    "__comment__start__": "npm run start to run in delevelopment mode.",
    "start": "node server.js",
    "dev": "webpack-dev-server --inline --hot",
    "__comment__build__": "npm run build to build production bundles",
    "build": "webpack --config webpack.config.prod.js -p",
    "lint": "eslint src/*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "mocha": "mocha --recursive --compilers js:babel-core/register -r mock-local-storage src/**/**/*.test.js src/**/*.test.js",
    "mocha:watch": "npm run mocha -- --watch",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --recursive --compilers js:babel/register --colors --reporter dot test/"
  },
  "keywords": [
    "webpack",
    "react",
    "workflow",
    "reload",
    "hot",
    "babel"
  ],
  "author": "ipland <ipland630@gmail.com>",
  "license": "ISC",
